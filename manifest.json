{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "CC-Icon16.png",
      "default_popup": "popup.html",
      "default_title": "COVID CLEANER"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "jquery-2.1.4.min.js", "content.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "description": "Clean your page of all instances of COVID and related words",
   "icons": {
      "128": "CC-Icon128.png",
      "16": "CC-Icon16.png",
      "48": "CC-Icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Covid Cleaner - COVID-19-less Internet",
   "options_ui": {
      "open_in_tab": false,
      "page": "options.html"
   },
   "permissions": [ "storage", "activeTab" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1"
}
